@using TeaCommerce.Api.Models
@using TeaCommerce.Umbraco.Web

@{
  Order currentOrder = TC.GetCurrentOrder( long.Parse( Model._store ) );
  string html = new WebClient().DownloadString( currentOrder.Properties[ "teaCommerceCummunicationUrl" ] + "&teaCommerceCommunicationType=checkout" );
}

@Html.Raw( html )